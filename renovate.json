{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>opencastsoftware/renovate-config"],
  "packageRules": [
    {
      "matchPackageNames": "mongo",
      "allowedVersions": "<4.5"
    },
    {
      "matchDatasources": ["sbt-package", "sbt-plugin"],
      "registryUrls": [
        "https://repo.maven.apache.org/maven2/",
        "https://repo.scala-sbt.org/scalasbt/sbt-plugin-releases/",
        "https://open.artefacts.tax.service.gov.uk/maven2",
        "https://open.artefacts.tax.service.gov.uk/ivy2"
      ]
    }
  ]
}
